<Root>
  <commands>
    <cmd type="radio" group="IM" name="IM_Off" _label="_Off"/>
    <cmd type="radio" group="IM" name="IM_Vni" _label="_Vni"/>
    <cmd type="radio" group="IM" name="IM_Telex" _label="_Telex"/>
    <cmd type="radio" group="IM" name="IM_Viqr" _label="V_iqr"/>
    <cmd type="radio" group="IM" name="IM_StarViqr" _label="Vi_qr*"/>

    <cmd type="radio" group="CS" name="CS_Unicode" _label="_Unicode"/>
    <cmd type="radio" group="CS" name="CS_Tcvn3" _label="_Tcvn3"/>
    <cmd type="radio" group="CS" name="CS_Vni" _label="_Vni"/>
    <cmd type="radio" group="CS" name="CS_Viqr" _label="V_iqr"/>
    <cmd type="radio" group="CS" name="CS_Viscii" _label="Vi_scii"/>
    <cmd type="radio" group="CS" name="CS_Vps" _label="V_ps"/>

    <cmd name="Enable" _label="_Enable" type="toggle" />
    <cmd name="Spell" _label="_Spelling Checking" type="toggle" />

    <cmd name="Props" _label="_Preferences..." pixtype="stock" pixname="gtk-properties" />
    <cmd name="About" _label="_About..." pixtype="stock" pixname="gnome-stock-about" />
  </commands>
  <popups>
    <popup name="button3">
      <submenu name="Input Method" _label="_Input Method">
        <menuitem name="Item 1" verb="IM_Off" />
        <menuitem name="Item 2" verb="IM_Vni" />
        <menuitem name="Item 3" verb="IM_Telex" />
        <menuitem name="Item 4" verb="IM_Viqr" />
        <menuitem name="Item 5" verb="IM_StarViqr"/>
      </submenu>
      <submenu name="Charset" _label="_Charset">
        <menuitem name="Item 1" verb="CS_Unicode" />
        <menuitem name="Item 2" verb="CS_Tcvn3" />
        <menuitem name="Item 3" verb="CS_Vni" />
        <menuitem name="Item 4" verb="CS_Viqr" />
        <menuitem name="Item 5" verb="CS_Viscii" />
        <menuitem name="Item 6" verb="CS_Vps" />
      </submenu>
      <menuitem name="Item 0" verb="Enable" />
      <menuitem name="Item 0" verb="Spell" />

      <menuitem name="Item 1" verb="Props" />
      <menuitem name="Item 3" verb="About" />
    </popup>
  </popups>
</Root>
